{% extends "base.html" %}
{% load core %}{% load markup %}{% load advertising %}{% load thumbnail %}{% load comparison %}
{% block head_extras %}
<link rel="stylesheet" type="text/css" href="/media/style/2009-06/style/secondfront.css" />
<script type="text/javascript" src="/media/style/2009-06/js/jquery.tabswitch.yui.js"></script>
<link rel="stylesheet" type="text/css" href="/media/style/2009-07/js/calendaring.css" />
{% comment %}<script type="text/javascript" src="/media/style/2009-07/js/calendaring.js"></script>{% endcomment %}
{% comment %}<script type="text/javascript" src="/media/style/2009-06/js/SlideTab.js"></script>
<script type="text/javascript" src="/media/style/2009-06/js/TopOnlineTab.js"></script>
<script type="text/javascript" src="/media/style/2009-06/js/NewsTab.js"></script>{% endcomment %}

<script type="text/javascript">
	$(function(){
		$('.SlideTab').tabSwitch('create',{width: 622, type: 'toggle', toggle: 'fade', height: 370});
    {% if not featured_multimedia %}
		$('.SlideTab').tabSwitch('startAuto',{speed: 400, interval: 6000});
    {% else %}{% ifnotequal featured_multimedia.mediatype "slideshow" %}{% ifnotequal featured_multimedia.mediatype "video" %}
        $('.SlideTab').tabSwitch('startAuto',{speed: 400, interval: 6000});
    {% endifnotequal %}{% endifnotequal %}{% endif %}
		$('.tabSelect').click(function(e){
			$('.SlideTab').tabSwitch('moveTo',{index: parseInt($(this).attr("rel"))});
			e.preventDefault();
		});
		$('.Nav').click(function(e){
		    $('.SlideTab').tabSwitch('moveStep',{step: parseInt($(this).attr("rel"))});
			e.preventDefault();
		});
	});
</script>

<script type="text/javascript">
	$(function(){
		$('.TopOnlineTab').tabSwitch('create',{width: 300, type: 'toggle', toggle: 'fade', height: 280});
		$('.tabSwap').click(function(e){
			$('.TopOnlineTab').tabSwitch('moveTo',{index: parseInt($(this).attr("rel"))});
			e.preventDefault();
		});
	});
</script> 

<script type="text/javascript">
	$(function(){
		$('.NewsTab').tabSwitch('create',{width: 300, type: 'toggle', toggle: 'fade', height: 300});
		$('.tabChange').click(function(e){
			$('.NewsTab').tabSwitch('moveTo',{index: parseInt($(this).attr("rel"))});
			e.preventDefault();
		});
	});
</script>
{% comment %}{% endcomment %}
{% endblock head_extras %}
{% block rss_link %}http://www.themaneater.com/media/style/2009-07/images/maneater/rss_20.png{% endblock rss_link %}
{% block facebook_link %}http://www.themaneater.com/media/style/2009-07/images/maneater/facebook_20.png{% endblock facebook_link %}
{% block twitter_link %}http://www.themaneater.com/media/style/2009-07/images/maneater/twitter_20.png{% endblock twitter_link %}
{% block title %}
{{ block.super }} &ndash; New Front Page
{% endblock %}
{% block content_wide %}
{% if election %}{{ election }}{% endif %}
{% if news_bursts %}
    {% for burst in news_bursts %}
    <div id="news_burst_container" class="grid_12"{% if burst.burst_type.label_color %} style="background-color:{{ burst.burst_type.label_color }};"{% endif %}>
        <div id="news_burst_label" class="grid_2 alpha omega">{% if burst.burst_type.burst_type_name %}{{ burst.burst_type.burst_type_name }}:{% else %}Breaking News:{% endif %}</div>
        <div id="news_burst_content" class="grid_10 alpha omega"{% if burst.burst_type.content_color %} style="background-color:{{ burst.burst_type.content_color }};"{% endif %}>
            <ul>
                <li>
                    <h1>{% if burst.link %}<a href="{{ burst.link }}">{% endif %}{{ burst.title }}{% if burst.link %}</a>{% endif %}</h1>
                    <p>{{ burst.body }}</p>
                </li>
            </ul>
        </div>
    </div>
    {% endfor %}
{% endif %}
{% if other_info %}
    <div id="news_burst_container" class="grid_12">
        <div id="news_burst_label" class="grid_2 alpha omega">
            <p>Special Information:</p>
        </div>
        <div id="news_burst_content" class="grid_10 alpha omega">
            <p>{{ other_info }}</p>
        </div>
    </div>
{% endif %}
<div id="top_stories" class="grid_12 fp_row">
    <div id="fp_section_articles" class="grid_8 alpha">
    {% if featured_multimedia %}
    <div class="SlideTab" id="TopStory0">
        <div id="fp_section" class="grid_5 alpha">
           <div id="top_story">
                <div class="article_info">
                    {% ifequal featured_multimedia.mediatype "custom" %}
                        {% thumbnail featured_multimedia.photo.image 380x255 as thumb %}
                            <div class="top_story_image_0" style="width: {{ thumb.width }}px;">
                        	    {% if featured_multimedia.get_absolute_url %}
                        	        <a href="{{ featured_multimedia.get_absolute_url }}">
                    	        {% endif %}
                    	        <img id="top_story_photo" src="{{ thumb }}" alt="Photo" class="photo" />
                    	        {% if featured_multimedia.get_absolute_url %}
                	                </a>
                	            {% endif %}
                	        </div>
                	{% else %}
                        {% ifequal featured_multimedia.mediatype "slideshow" %}
                            {% thumbnail featured_multimedia.image 380x255 as thumb %}
                                <div class="top_story_image_0" style="width: {{ thumb.width }}px;">
                                    <a href="{{ featured_multimedia.get_absolute_url }}">
                                        <img id="top_story_photo" src="{{ thumb }}" alt="Slideshow" class="photo" />
                                    </a>
                                </div>
                        {% else %}
                            {% ifequal featured_multimedia.mediatype "video" %}
                                <object width="420" height="383">
                                    <param name="movie" value="{{ featured_multimedia.swf }}"></param>
                                    <embed src="{{ featured_multimedia.swf }}" type="application/x-shockwave-flash" width="380" height="255"></embed>
                                </object>
                            {% endifequal %}
                        {% endifequal %}
                    {% endifequal %}
                    <h1>
                        {% if featured_multimedia.get_absolute_url %}
                            <a href="{{ featured_multimedia.get_absolute_url }}">
                        {% endif %}
                        {{ featured_multimedia.title|markdown }}
                        {% if featured_multimedia.get_absolute_url %}
                            </a>
                        {% endif %}
                    </h1>
                    <div class="top_article_cdeck">
                        {% if featured_multimedia.description %}
                            {{ featured_multimedia.description|markdown }}
                        {% endif %}
                    </div>
                </div>
            </div>
        </div>
        <div id="fp_section" class="grid_3 alpha">
            <div class="othertoplinks">
                <ul class="otl">
                    <noscript><div class="otltitle">Top Stories:</div></noscript>
                    <li class="otlitem active">
                        <a class="tabSelect" href="#TopStory0" rel="0">
                            <img src="{% ifequal featured_multimedia.mediatype "video" %}{{ featured_multimedia.thumbnail }}{% else %}{% ifequal featured_multimedia.mediatype "slideshow" %}{% thumbnail featured_multimedia.image 60x45 crop %}{% else %}{% thumbnail featured_multimedia.photo.image 60x45 crop %}{% endifequal %}{% endifequal %}" />
                            <div class="slider_text">
                                {{ featured_multimedia.title|markdown }}
                            </div>
                        </a>
                        <div class="clearer"></div>
                    </li>
                {% for article in centerpiece_data %}
                    <li class="otlitem">
                        <a class="tabSelect" href="#TopStory{{ forloop.counter }}" rel="{{ forloop.counter }}">
                            <img src="{% thumbnail article.photos.all.0.image 60x45 crop %}" />
                            <div class="slider_text">{{ article.title|markdown }}</div>
                        </a>
                        <div class="clearer"></div>
                    </li>
                {% endfor %}
                </ul>
            </div>
        </div>
    </div>
    {% endif %}
    {% for lead_article in centerpiece %}
    <div class="SlideTab" id="TopStory{% if featured_multimedia %}{{ forloop.counter }}{% else %}{{ forloop.counter0 }}{% endif %}">
        <div id="fp_section" class="grid_5 alpha">
            <div id="top_story">
                <div class="article_info">
                    {% if lead_article.photos.all %}
                        {% thumbnail lead_article.photos.all.0.image 380x255 as thumb %}
                        <div class="top_story_image_0" style="width: {{ thumb.width }}px;">
                            <a href="{{ lead_article.get_absolute_url }}">
                                <img id="top_story_photo" src="{{ thumb }}" alt="Photo" class="photo" />
                            </a>
                        </div>
                    {% endif %}
                <h1><a href="{{ lead_article.get_absolute_url }}">{{ lead_article.title|markdown }}</a></h1>
                <div class="top_article_cdeck">
                    {% if lead_article.blurb %}
                        {{ lead_article.blurb|markdown }}
                    {% else %}
                        {% if lead_article.cdeck %}
                            {{ lead_article.cdeck|markdown }}
                        {% else %}
                            {{ lead_article.body|markdown|truncatewords_html:"40" }}
                        {% endif %}
                    {% endif %}
                </div>
                {% if lead_article.updated %}
                    <p class="breaking">(Last updated {{ lead_article.last_updated|date:"g:i a, N j, Y" }})</p>
                {% endif %}
                </div>
            </div>
        </div>
        <div id="fp_section" class="grid_3 alpha">
            <div class="othertoplinks">
                <ul class="otl">
                    {% comment %}<div class="otltitle">Top Stories:</div>{% endcomment %}
                    {% if featured_multimedia %}
                        <li class="otlitem">
                            <a class="tabSelect" href="#TopStory0" rel="0">
                                <img src="{% ifequal featured_multimedia.mediatype "video" %}{{ featured_multimedia.thumbnail }}{% else %}{% ifequal featured_multimedia.mediatype "slideshow" %}{% thumbnail featured_multimedia.image 60x45 crop %}{% else %}{% ifequal featured_multimedia.mediatype "custom" %}{% thumbnail featured_multimedia.photo.image 60x45 crop %}{% endifequal %}{% endifequal %}{% endifequal %}" />
                                <div class="slider_text">{{ featured_multimedia.title|markdown }}</div>
                            </a>
                            <div class="clearer"></div>
                        </li>
                    {% endif %}
                    {% for article in centerpiece_data %}
                        <li class="otlitem{% ifequal lead_article article%} active{% endifequal %}">
                            <a class="tabSelect" href="#TopStory{% if featured_multimedia %}{{ forloop.counter }}{% else %}{{ forloop.counter0 }}{% endif %}" rel="{% if featured_multimedia %}{{ forloop.counter }}{% else %}{{ forloop.counter0 }}{% endif %}">
                                <img src="{% thumbnail article.photos.all.0.image 60x45 crop %}" />
                                <div class="slider_text">
                                    {{ article.title|markdown }}
                                </div>
                            </a>
                            <div class="clearer"></div>
                        </li>
                     {% endfor %}
                 </ul>
            </div>
        </div>
    </div>
    {% endfor %}    
    </div>
    
    <div id="top_other" class="grid_4 omega">
        <div class="tabbedarea">
        <div class="TopOnlineTab" id="TopOnline0">
            <div id="Mtab1" class="grid_2 alpha omega">
                <h2 class="title Mtab1">Most viewed</h2>
            </div>
            <div id="Mtab2" class="grid_2 alpha omega Mtabpassive">
                <h2 class="title Mtab2"><a class="tabSwap" href="#TopOnline1" rel="1">Other top stories</a></h2>
            </div>
            <p class="top_online">&raquo;Last updated {{ issue.pub_date|date:"N j, Y" }}</p>
            <ul>
                {% if top_online %}
                    {% for entry in top_online %}
                        <li><h2>{% if entry.prefix %}{{ entry.prefix }}: {% endif %}<a href="{{ entry.url }}">{{ entry.item_name|markdown }}</a></h2></li>
                    {% endfor %}
                {% else %}
                    <li><h2>D'oh!</h2></li>
                    <li><h2>Top online hasn't been entered for this issue yet.</h2></li>
                    <li><h2>FIX IT!</h2></li>
                    <li><h2>Today!</h2></li>
                {% endif %}
                {% comment %}
                <h2>Tag: <a href="/tags/best-of-2009/">Best Of 2009</a></h2>
                <h2>Tag: <a href="/tags/most-influential-students-2009/">Most Influential Students 2009</a></h2>
                <h2><a href="/stories/2009/3/12/-dreams-new-album-hit/">The-Dream's new album is a hit</a></h2>
                <h2><a href="/stories/2009/5/8/final-exams-causes-students-have-stress-bad-habits/">Final exams causes students to have stress, bad habits</a></h2>
                <h2><a href="/stories/2009/5/22/panhandle-eastern-pipeine-ruptures-howard-county/">Pipeline ruptures in Howard County</a></h2>
                <h2><a href="/stories/2009/5/8/seniors-celebrate-tiger-prowl/">Seniors celebrate at Tiger Prowl</a></h2>
                <h2><a href="/stories/2009/5/21/passage-bills-closes-out-legislative-session">Passage of bills closes out legislative session</a></h2>
                {% endcomment %}
            </ul>
        </div>
        <div class="TopOnlineTab" id="TopOnline1">
            <div id="Mtab1" class="grid_2 alpha omega Mtabpassive">
                <h2 class="title Mtab1"><a class="tabSwap" href="#TopOnline0" rel="0">Most viewed</a></h2>
            </div>
            <div id="Mtab2" class="grid_2 alpha omega">
                <h2 class="title Mtab2">Other top stories</h2>
            </div>
            <ul>
            {% for article in top_articles %}
                <li><h2>{% ifnotequal article.type 'story' %}{{ article.get_type_display }}: {% endifnotequal %}<a href="{{ article.get_absolute_url }}">{{ article.title|markdown }}</a></h2></li>
            {% endfor %}
            </ul>
        </div>
        </div>
        <div id="fp_mam_ad">
            <div><img src="/media/style/2009-01/style/ad_header.png" alt="Advertisement" /></div>
            <a href="http://www.myapartmentmap.com/"><p>Find off-campus apartments at <br />My Apartment Map</p>
            <img src="/media/2009/0115/mam-mini-logo.png" alt="My Apartment Map" /></a>
        </div>
    </div>
</div>
<div id="fp_online_content" class="grid_12 fp_row">
   
    <div id="fp_latest_mm" class="grid_6 alpha">
        <h2 class="section">Latest multimedia</h2>
        <div id="fp_other_content" class="grid_2 alpha">
            <h3>Latest {{ latest_video.mediatype|lower }}</h3>
            <h3><a href="{{ latest_video.get_absolute_url }}">{{ latest_video.title }}</a></h3
            <img src="{{ latest_video.thumbnail }}" alt="Link to video" class="right photo" />
            <p>{{ latest_video.description|markdown|truncatewords_html:"10" }} (<a href="{{ latest_video.get_absolute_url }}">View {{ latest_video.mediatype }}</a>{% if latest_video.article_set.all %} or <a href="{{ latest_video.article_set.all.0.get_absolute_url }}">read the article</a>{% endif %})</p>
            <div style="clear:both;"></div>
        </div>
        {% with latest_slideshow as latest_multimedia %}
        <div id="fp_other_content" class="grid_2 alpha">
            <h3>Latest {{ latest_multimedia.mediatype|lower }}</h3>
            <h3><a href="{{ latest_multimedia.get_absolute_url }}">{{ latest_multimedia.title }}</a></h3
            <img src="{% ifequal latest_multimedia.mediatype 'video' %}{{ latest_multimedia.thumbnail }}{% else %}{% thumbnail latest_multimedia.image 140x105 crop %}{% endifequal %}" alt="Link to {% ifequal latest_multimedia.mediatype 'video' %}video{% else %}slideshow{% endifequal %}" class="right photo" />
            <p>{{ latest_multimedia.description|markdown|truncatewords_html:"10" }} (<a href="{{ latest_multimedia.get_absolute_url }}">{% ifequal latest_multimedia.mediatype "audioclip" %}Listen to audio clip{% else %}View {{ latest_multimedia.mediatype }}{% endifequal %}</a>{% if latest_multimedia.article_set.all %} or <a href="{{ latest_multimedia.article_set.all.0.get_absolute_url }}">read the article</a>{% endif %})</p>
            <div style="clear:both;"></div>
        </div>
        {% endwith %}
        <div id="fp_other_content" class="grid_2 alpha">
            <h3>Latest podcasts</h3>
            <ul>
                {% for episode in latest_podcast_episodes %}
                <li><h3><a href="{{ episode.get_absolute_url }}">{{ episode }}</a></h3></li>
                {% endfor %}
            </ul>
            <p class="fp_section_refer"><a href="http://www.themaneater.com/podcasts/">See more podcasts...</a></p>
        </div>
    </div>
    <div id="fp_latest_blog_posts" class="grid_4">
        <div id="blogs_onlineonly_tabs">
        <div class="NewsTab" id="BlogOnline0">
            <div id="Mtab1" class="grid_2 alpha omega">
                <h2 class="Mtab1">Latest blog posts</h2>
            </div>
            <div id="Mtab2" class="grid_2 alpha omega Mtabpassive">
                <h2 class="Mtab2"><a class="tabChange" href="#BlogOnline1" rel="1">Web exclusives</a></h2>
            </div>
        {% if latest_blog_posts %}
        <ul>
            {% for post in latest_blog_posts %}
            <li>
                <h3><a href="{{ post.get_absolute_url }}">{{ post|markdown }}</a></h3>
                {{ post.body|markdown|truncatewords_html:"10" }}
                <p>(from <a href="{{ post.blog.get_absolute_url }}">{{ post.blog }}</a>)</p>
            </li>
            {% endfor %}
        </ul>
        <p class="fp_section_refer"><a href="http://www.themaneater.com/blogs/">See more blog posts...</a></p>
        {% else %}
                <p>There are currently no blog posts to display.</p>
                <p class="fp_section_refer"><a href="http://www.themaneater.com/blogs/">See more blog posts...</a></p>
        {% endif %}
        </div>
        <div class="NewsTab" id="BlogOnline1">
        	<div id="Mtab1" class="grid_2 alpha omega Mtabpassive">
        	    <h2 class="Mtab1"><a class="tabChange" href="#BlogOnline0" rel="0">Latest blog posts</a></h2>
        	</div>
        	<div id="Mtab2" class="grid_2 alpha omega">
        	    <h2 class="Mtab2">Web exclusives</h2>
        	</div>

        <ul>
            {% for article in latest_online_exclusives %}
            <li>
                <h3><a href="{{ article.get_absolute_url }}">{{ article.title|markdown }}</a></h3>
                {% if article.cdeck %}{{ article.cdeck|markdown }}{% else %}{{ article.body|markdown|truncatewords_html:"20" }}{% endif %}
                {% if article.updated %}<p class="breaking">last updated {{ article.last_updated|date:"g:i a, N j, Y" }}</p>{% endif %}
            </li>
            {% endfor %}
        </ul>
        </div>
    </div>
    </div>
    <div id="fp_latest_podcasts" class="grid_2 omega">
        <h2 class="section">Police blotter</h2>
        <ul>
            <li>Entries for the MUPD blotter will show up here when they are available.</li>
            <li>Needless to say, this should happen very soon.</li>
        </ul>
    </div>
</div>
<div id="ad_bar" class="grid_12 fp_row">
    <span class="right">{% render_adsense 'banner' %}</span>{% render_banner_ad 'banner' %}
</div>
<div id="fp_articles_calendar" class="grid_12 fp_row">
    <div id="fp_section_more_articles" class="grid_9 alpha">
        {% for row in all_sections|columns:"3" %}
        <div id="fp_section_articles_row{% if not forloop.last %}1{% else %}2{% endif %}" class="grid_9 alpha">
            {% for item in row %}
            {% comment %}Sections have slugs, but issues don't.{% endcomment %}
            <div id="fp_section" class="grid_3 {% cycle 'alpha' 'beta' 'omega' %}">
                {% if item.slug %}
                <h2 class="section"><a href="{{ item.get_absolute_url }}">{{ item.name }}</a></h2>
                <ul>
                    {% for article in item.articles %}
                    <li><h3>{% ifnotequal article.type 'story' %}{{ article.get_type_display }}: {% endifnotequal %}<a href="{{ article.get_absolute_url }}">{{ article.title|markdown }}</a></h3></li>
                    {% endfor %}
                </ul>
                <p class="fp_section_refer"><a href="{{ item.get_absolute_url }}">See more articles in {{ item.name }}...</a></p>
                {% else %}
                {% if item.0.slug %}
                 <h2 class="sectionmove"><a href="http://move.themaneater.com/">MOVE</a></h2>
                    <ul>
                    {% for article in item %}
                    <li><h3 class="move">{% ifnotequal article.type 'story' %}{{ article.get_type_display }}: {% endifnotequal %}<a href="{{ article.get_absolute_url }}">{{ article.title|markdown }}</a></h3></li>
                    {% endfor %}
                    </ul>
                 <p class="fp_section_refer"><a class="move" href="http://move.themaneater.com/">See more of MOVE...</a></p>
                {% else %}
                <h2 class="section"><a href="{{ item.get_absolute_url }}">In print</a></h2>
                <ul>
                    {% if issue_front %}<li class="issue_front_thumb"><a href="{{ issue_front.get_absolute_url }}"><img class="photo left" src="{{ issue_front.get_thumbnail200_url }}" alt="View the front page" /></a></li>{% endif %}
                    {% if item.comicepisode_set.all %}<li><h3><a href="/comics/latest/">Comics</a></h3></li>{% endif %}
                    <li><h3><a href="/games/">Games</a></h3></li>
                    {% for special_section in item.specialsection_set.all %}
                    <li><h3><a href="{{ special_section.get_absolute_url }}">{{ special_section }}</a></h3></li>
                    {% endfor %}
                    {% for layout in issue_other_layouts %}
                    <li><a href="{{ layout.get_absolute_url }}">{% if not layout.last_page %}Page {{ layout.first_page }}{% else %}Pages {{ layout.first_page }}&ndash;{{ layout.last_page }}{% endif %}</a></li>
                    {% endfor %}
                </ul>
                <p class="fp_section_refer" style="clear: both;"><a href="{{ item.get_absolute_url }}">See more from this issue...</a></p>
                {% comment %}{% if blotter_entries %}
                <h2 class="section">Latest blotter entries</h2>
                <ul>
                    {% for incident in blotter_entries %}
                    <li><a href="{{ incident.get_absolute_url }}">{{ incident.incident_date|date:"P, N j, Y" }}</a></li>
                    {% endfor %}
                </ul>
                <p class="fp_section_refer"><a href="/blotter/">See more blotter entries...</a></p>
                {% endif %}{% endcomment %}
                {% endif %}
                {% endif %}
            </div>
            {% endfor %}
        </div>
        {% endfor %}
    </div>
    <div id="fp_calendar" class="grid_3 omega">
        <h2 class="section calendar"><a href="/calendar/">Calendar &ndash; {{ month_name }} {{ year }}</a></h2>
{% if month_formatted %}
<table class="calendar">
{% if weekday_header %}
<thead>
<tr class="header">
{% for weekday_name in weekday_header %}
<th class="header">
{{ weekday_name }}
</th>
{% endfor %}
</tr>
</thead>
{% endif %}
